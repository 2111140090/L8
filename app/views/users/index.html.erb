<h1>登録ユーザ一覧</h1>

<table>
  <thead>
    <tr>
      <th>ユーザー名</th>
    　<th>ユーザー削除</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.uid %></td>
            <td><%= button_to '削除', "/users/#{user.uid}", method: :get %></td>
        </tr>
    <% end %>
  </tbody>
</table>


<p>
    <%= button_to '新規ユーザー登録へ', users_new_path, method: :get %><br>
    <%= button_to 'ホーム', root_path, method: :get %>
</p>