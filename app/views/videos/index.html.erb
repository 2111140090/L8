YouTube<br>

<% @videos.each do |e| %>

    <p>
        <%= e.title %><br>
        <%= e.introduction %><br>
        <%= link_to "再生", video_path(e) %><br>
        <%= @like_count %>
        <%= link_to 'いいね', likes_path(video_id: e.id), method: :post %>
        <%= link_to 'いいね削除', like_path(e), method: :delete %>
    </p>
<% end %>

<%= link_to "動画投稿する", new_video_path %>