<h1>動画の投稿</h1>

<%= form_with model: @video, local: true do |f| %>
  <table>
    <tr><td>タイトル</td><td><%= f.text_field :title %></td></tr>
    <tr><td>概要</td><td><%= f.text_area :introduction %></td></tr>
    <tr><td>動画</td><td><%= f.file_field :video %></td></tr>
  </table>

  <%= f.submit "投稿する" %>
<% end %>
<br>
<%= button_to 'ホーム', root_path, method: :get %>
